<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cs196game"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.cs196game.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="sensorLandscape"
            android:theme="@style/NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.cs196game.GameActivity"
            android:label="@string/title_activity_game"
            android:screenOrientation="sensorLandscape"
            android:theme="@style/NoActionBar" >
        </activity>
        <activity
            android:name="com.example.cs196game.HighScoreList"
            android:label="@string/title_activity_high_score_list"
            android:screenOrientation="sensorLandscape"
            android:theme="@style/NoActionBar" >
        </activity>
        <activity
            android:name="com.example.cs196game.Credits"
            android:label="@string/button_credits"
            android:screenOrientation="sensorLandscape"
            android:theme="@style/NoActionBar" >
        </activity>
        <activity
            android:name="com.example.cs196game.Settings"
            android:label="@string/title_activity_settings"
            android:screenOrientation="sensorLandscape"
            android:theme="@style/NoActionBar" >
        </activity>
    </application>

</manifest>
